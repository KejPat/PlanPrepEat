\r\n        return <SectionImage key={recipe._id} name={recipe.name} caption={recipe.description} img={\"http://localhost:5000/\" + recipe.image} id={recipe._id} />\r\n    })\r\n\r\n    // const prepRecipes = recentlyAdded.map(recipe => {\r\n    //     return <WideImage key={recipe.key} name={recipe.name} img={recipe.image} />\r\n    // })\r\n        \r\n    return (\r\n        <Aux>\r\n             <p className={classes.Category}>Recently Added</p>\r\n            <div className={classes.RecentlyAddedImages}>\r\n                {recentlyAddRecipes}\r\n            </div>\r\n            <div className={classes.Combine}>\r\n                <p className={classes.Category}>Dinner</p>\r\n                <Link className={classes.ViewMore} to=\"/Dinner\">View More...</Link>\r\n            </div>\r\n            <div className={classes.SectionImages}>\r\n                {dinnerRecipes}\r\n            </div>\r\n            {/* {prepRecipes} */}\r\n            <div className={classes.Combine}>\r\n                <p className={classes.Category}>Lunch</p>\r\n                <Link className={classes.ViewMore} to=\"/Lunch\">View More...</Link>\r\n            </div>\r\n            <div className={classes.SectionImages}>\r\n                {lunchRecipes}\r\n            </div> \r\n        </Aux>\r\n    );\r\n}\r\n\r\nexport default Homepage;","C:\\Users\\kejal\\Documents\\Software\\foodblog\\src\\hoc\\Auxiliry\\Auxiliry.js",[],"C:\\Users\\kejal\\Documents\\Software\\foodblog\\src\\Component\\Navigation\\Navbar\\NavBar.js",[],"C:\\Users\\kejal\\Documents\\Software\\foodblog\\src\\Component\\Navigation\\DropDown\\DropDown.js",[],"C:\\Users\\kejal\\Documents\\Software\\foodblog\\src\\Component\\Navigation\\NavItems\\NavItems.js",[],"C:\\Users\\kejal\\Documents\\Software\\foodblog\\src\\Component\\Navigation\\Background\\Background.js",[],"C:\\Users\\kejal\\Documents\\Software\\foodblog\\src\\Component\\Navigation\\DropDown\\MenuIcon\\MenuIcon.js",[],"C:\\Users\\kejal\\Documents\\Software